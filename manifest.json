{
  "manifest_version": 3,
  "name": "Google Meet Transcript Exporter",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon16.ico",
      "48": "img/icon48.ico",
      "128": "img/icon128.ico"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["js/contentScript.js"]
    }
  ]
}
